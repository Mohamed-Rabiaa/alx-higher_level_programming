#!/usr/bin/python3
def max_integer(my_list=[]):
    if not list:
        return None
    max_num = 0
    length = len(my_list)
    while i < length - 1 :
        if my_list[i] > my_list[i + 1]:
            max_num = my_list[i]
        elif my_list[i + 1] > my_list[i]:
            max_num = my_list[i + 1]
        i -= 1
    return max_num
